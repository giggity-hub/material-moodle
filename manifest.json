{
  "manifest_version": 2,
  "name": "Material Moodle",
  "version": "0.0.1",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "description": "Better looking Moodle",

  "content_scripts": [
    {
      "matches": ["https://moodle.uni-due.de/*"], 
      "run_at": "document_start",
      "js": [
        "lib/jquery-3.2.1.min.js",
        "dist/main.js" 
      ]
    }
  ]

}


